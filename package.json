{
  "name": "scrollbear",
  "version": "0.1.0",
  "author": "Mikita Kukavenka",
  "license": "MIT",
  "scripts": {
    "webpack-build": "webpack",
    "webpack-watch": "webpack --watch",
    "server": "cd dist && lite-server",
    "clean": "rm -rf dist/*",
    "serve": "yarn clean && cp src/index.html dist/index.html && yarn server",
    "dev-build": "yarn clean && cp src/index.html dist/index.html && cp CNAME dist/CNAME && yarn webpack-build",
    "dev-deploy": "yarn dev-build && cd dist && git add --all && git commit -m \"Dev-build at $(date)\" && git push origin gh-pages --force",
    "start": "webpack-dev-server --hot --open"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "1.5.2",
    "babel-preset-react": "6.24.1",
    "html-webpack-plugin": "^3.1.0",
    "lite-server": "^2.3.0",
    "material-ui": "next",
    "material-ui-icons": "^1.0.0-beta.36",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-swipeable-views": "^0.12.13",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  }
}
